{
    "extends": ["airbnb", "prettier"],
    "plugins": ["prettier"],
    "rules": {
      "prettier/prettier": ["error"],
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "react/jsx-props-no-spreading": [1, {
        "html": "ignore" ,
        "custom": "ignore" ,
        "explicitSpread": "ignore"
    }]
    }
}